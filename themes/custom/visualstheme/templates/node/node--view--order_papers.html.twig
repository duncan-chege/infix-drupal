 <div style="display: grid; grid-template-columns: 1fr 1fr 3fr 1fr 0.8fr; gap: 15px 10px; overflow-x:auto;">
   <p style="font-size: 14px;"> {{ node.field_date_passed.value|date("j F Y") }} </p>
   <p class="fw-semibold"> {{ node.field_order_num.value }} </p>
   <p style="width: 450px;"> {{ node.title.value }} </p>
   <p style="font-size: 14px;"> {{ node.field_house.value }} </p>
   <a href="{{ path('entity.node.canonical', {'node': node.id}) }}" class="">View</a>
 </div>
