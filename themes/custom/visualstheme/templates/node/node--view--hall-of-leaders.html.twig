<div class="doc-row" class="col-md-2 text-center" data-parliament="{{ node.field_parliament_number.value }}">
  {% set image_url = file_url(node.field_mp_image.entity.getFileUri())  %}
  {% if image_url %}
  <img src="{{ image_url }}" alt="{{ node.field_images.alt }}">
  {% else %}
  <img src="{{ base_path ~ directory }}/images/mp-placeholder.jpg">
  {% endif %}
  
  <p class="mt-2 mb-0">{{ label }}</p>
  <p class="mb-2">{{ node.field_constituency.value }}</p>
</div>
